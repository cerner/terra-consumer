{
  "name": "terra-consumer-site",
  "version": "0.4.22",
  "description": "Documentation Site for Functional Verification",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerner/terra-consumer.git"
  },
  "author": "Cerner Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cerner/terra-consumer/issues"
  },
  "homepage": "https://github.com/cerner/terra-consumer#readme",
  "scripts": {
    "start": "webpack-dev-server --progress",
    "compile": "npm run compile:clean && npm run compile:build",
    "compile:clean": "$(cd ..; npm bin)/rimraf lib",
    "compile:build": "$(cd ..; npm bin)/babel src --out-dir lib --copy-files",
    "compile:heroku": "webpack --config webpack.prod.config --progress",
    "compile:prod": "webpack --config webpack.prod.config -p",
    "compile:prod-stats": "webpack --config webpack.prod.config -p --json > stats.json",
    "deploy": "npm run compile:prod && gh-pages -d build",
    "lint": "npm run lint:js",
    "lint:js": "$(cd ..; npm bin)/eslint --ext .js,.jsx . --ignore-path ../../.eslintignore",
    "test": "npm run test:spec",
    "test:spec": "echo \"Warning: no test written\" && exit 0"
  },
  "dependencies": {
    "moment": "^2.20.1",
    "prop-types": "^15.6.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^3.2.0",
    "redux": "^3.7.2",
    "terra-arrange": "^2.0.0",
    "terra-base": "^3.0.0",
    "terra-button": "^2.0.0",
    "terra-consumer-icon": "^2.0.0",
    "terra-consumer-layout": "^1.0.1",
    "terra-consumer-nav": "^1.0.1",
    "terra-form": "^2.0.0",
    "terra-grid": "^4.0.0",
    "terra-i18n": "^2.0.0",
    "terra-i18n-plugin": "^2.0.0",
    "terra-icon": "^2.0.0",
    "terra-list": "^2.0.0",
    "terra-markdown": "^2.0.0",
    "terra-props-table": "^2.0.0",
    "terra-table": "^2.0.0"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "node-sass": "^4.7.2",
    "postcss-custom-properties": "^6.2.0",
    "postcss-loader": "^2.1.0",
    "postcss-rtl": "^1.2.3",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  }
}
