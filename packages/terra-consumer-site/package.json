{
  "name": "terra-consumer-site",
  "version": "0.4.1",
  "description": "Documentation Site for Functional Verification",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerner/terra-consumer.git"
  },
  "author": "Cerner Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cerner/terra-consumer/issues"
  },
  "homepage": "https://github.com/cerner/terra-consumer#readme",
  "scripts": {
    "start": "webpack-dev-server --progress",
    "compile": "npm run compile:clean && npm run compile:build",
    "compile:clean": "$(cd ..; npm bin)/rimraf lib",
    "compile:build": "$(cd ..; npm bin)/babel src --out-dir lib --copy-files",
    "compile:heroku": "webpack --config webpack.prod.config --progress",
    "compile:prod": "webpack --config webpack.prod.config -p",
    "compile:prod-stats": "webpack --config webpack.prod.config -p --json > stats.json",
    "deploy": "npm run compile:prod && gh-pages -d build",
    "lint": "npm run lint:js",
    "lint:js": "$(cd ..; npm bin)/eslint --ext .js,.jsx . --ignore-path ../../.eslintignore",
    "test": "npm run test:spec",
    "test:spec": "echo \"Warning: no test written\" && exit 0"
  },
  "dependencies": {
    "moment": "^2.17.1",
    "prop-types": "^15.5.8",
    "react-intl": "^2.3.0",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.5",
    "redux": "^3.6.0",
    "terra-arrange": "^1.7.0",
    "terra-base": "^2.1.0",
    "terra-button": "^1.6.0",
    "terra-consumer-icon": "^1.2.1",
    "terra-consumer-layout": "^0.1.0-BETA.1",
    "terra-consumer-nav": "^0.1.0-BETA.5",
    "terra-form": "^1.6.0",
    "terra-grid": "^3.10.0",
    "terra-i18n": "^1.5.0",
    "terra-i18n-plugin": "^1.2.0",
    "terra-icon": "^1.7.0",
    "terra-list": "^1.7.0",
    "terra-markdown": "^1.5.0",
    "terra-props-table": "^1.5.0",
    "terra-table": "^1.7.0"
  },
  "peerDependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.29.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.3",
    "postcss-custom-properties": "^6.1.0",
    "postcss-loader": "^2.0.6",
    "postcss-rtl": "^0.5.10",
    "raw-loader": "^0.5.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1"
  }
}
